{
  "name": "brianzotoole",
  "version": "3.1.0",
  "description": "portfolio site using jeckyll and webpack",
  "main": "webpack.dev.js",
  "browserslist": [
    "> 1%",
    "ie >= 10"
  ],
  "scripts": {
    "dev": "webpack --config webpack.dev.js",
    "start": "webpack --watch --config webpack.dev.js",
    "jekyll": "jekyll s --destination dist",
    "jekyll-prod": "JEKYLL_ENV=production jekyll build --destination dist",
    "webpack-prod": "NODE_ENV=production webpack --config webpack.prod.js",
    "prod": "npm run webpack-prod && npm run jekyll-prod && http-server ./dist"
  },
  "dependencies": {
    "fullpage.js": "^3.0.5",
    "gsap": "^1.20.3",
    "jquery": "^3.4.0",
    "scrollmagic": "^2.0.7",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^7.1.5",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^1.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "http-server": "^0.10.0",
    "lodash": ">=4.17.11",
    "node-sass": "^4.12.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dashboard": "^3.3.7",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.2.1",
    "webpack-notifier": "^1.8.0"
  },
  "author": "",
  "license": "ISC"
}
